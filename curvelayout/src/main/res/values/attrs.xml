<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CurveLayout">
        <attr name="curveHeight" format="dimension" />
        <attr name="curvesGravity" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="curveFormat" format="enum">
            <enum name="convex" value="1" />
            <enum name="concave" value="2" />
        </attr>

    </declare-styleable>
</resources>