<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">CurveLayout</string>
    <declare-styleable name="CurveLayout">
        <attr format="dimension" name="curveHeight"/>
        <attr format="enum" name="curvesGravity">
            <enum name="left" value="1"/>
            <enum name="top" value="2"/>
            <enum name="right" value="3"/>
            <enum name="bottom" value="4"/>
        </attr>
        <attr format="enum" name="curveFormat">
            <enum name="convex" value="1"/>
            <enum name="concave" value="2"/>
        </attr>

    </declare-styleable>
</resources>